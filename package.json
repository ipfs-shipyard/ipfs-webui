{
  "name": "ipfs-webui-next",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "cp config/webpack.config.prod.js node_modules/react-scripts/config/",
    "build": "react-scripts build",
    "build:icons": "svgr -d src/icons node_modules/ipfs-css/icons && standard --fix src/icons/*",
    "lint": "standard src",
    "test": "react-scripts test",
    "test:coverage": "npm run test:run -- --coverage\"",
    "test:serve": "ecstatic build --cache=0 --port=3001",
    "test:ci": "URL=http://localhost:3001 run-p --race test:serve test",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "cids": "^0.5.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "internal-nav-helper": "^1.0.2",
    "ipfs-css": "^0.4.0",
    "milliseconds": "^1.0.3",
    "multibase": "^0.4.0",
    "multicodec": "^0.2.6",
    "multihashes": "^0.4.13",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-debounce-render": "^4.0.1",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.2",
    "redux-bundler": "^21.2.0",
    "redux-bundler-react": "^1.0.0",
    "svgr": "^1.9.1",
    "tachyons": "^4.9.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "window-or-global": "^1.0.1",
    "window.ipfs-fallback": "^1.1.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.4.4",
    "@storybook/addon-actions": "^3.4.4",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/addons": "^3.4.4",
    "@storybook/react": "^3.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-runtime": "^6.26.0",
    "ecstatic": "^3.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.1",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^1.3.0",
    "source-map-explorer": "^1.5.0",
    "standard": "^11.0.1"
  },
  "homepage": "./",
  "standard": {
    "parser": "babel-eslint"
  }
}
